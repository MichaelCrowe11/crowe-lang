# Server Configuration
NODE_ENV=development
PORT=3001

# Database
MONGODB_URI=mongodb://localhost:27017/crowe-cloud
REDIS_URL=redis://localhost:6379

# Authentication
JWT_SECRET=your-secret-key-change-this-in-production
JWT_EXPIRE=7d

# Stripe Payment Processing
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret
STRIPE_PRICE_ID_PRO=price_pro_monthly
STRIPE_PRICE_ID_TEAM=price_team_monthly
STRIPE_PRICE_ID_ENTERPRISE=price_enterprise_monthly

# AWS (for file storage and deployment)
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-east-1
AWS_S3_BUCKET=crowe-cloud-storage

# Email Service (SendGrid)
SENDGRID_API_KEY=your_sendgrid_api_key
FROM_EMAIL=noreply@crowe-lang.dev

# AI Integration
OPENAI_API_KEY=your_openai_api_key
AI_MODEL=gpt-3.5-turbo

# Analytics
MIXPANEL_TOKEN=your_mixpanel_token
GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX

# Feature Flags
ENABLE_AI_FEATURES=true
ENABLE_TEAM_FEATURES=true
ENABLE_ENTERPRISE_FEATURES=false

# Rate Limiting
RATE_LIMIT_FREE=100
RATE_LIMIT_PRO=1000
RATE_LIMIT_TEAM=5000
RATE_LIMIT_ENTERPRISE=unlimited

# Cloud Resources Limits (per month)
BUILD_MINUTES_FREE=100
BUILD_MINUTES_PRO=1000
BUILD_MINUTES_TEAM=5000
BUILD_MINUTES_ENTERPRISE=unlimited

PROJECTS_LIMIT_FREE=3
PROJECTS_LIMIT_PRO=unlimited
PROJECTS_LIMIT_TEAM=unlimited
PROJECTS_LIMIT_ENTERPRISE=unlimited

# Support
SUPPORT_EMAIL=support@crowe-lang.dev
SUPPORT_SLACK_WEBHOOK=https://hooks.slack.com/services/YOUR/WEBHOOK/URL

# Security
ALLOWED_ORIGINS=http://localhost:3000,https://crowe-lang.dev
SESSION_SECRET=your-session-secret-change-this